{
  "extends": [
    "next/core-web-vitals",
    "next/typescript"
  ],
  "ignorePatterns": [
    "utilities-scripts/*"
  ],
  "rules": {
    "@typescript-eslint/no-explicit-any": "off",
    "indent": [
      "error",
      2
    ],
    "react/jsx-max-props-per-line": [
      2,
      {
        "maximum": 1
      }
    ]
  }
}